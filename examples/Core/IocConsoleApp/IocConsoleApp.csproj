<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="TouchSocket.CodeAnalyzer" Version="2.0.0-beta.208">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="TouchSocket.Core" Version="2.0.0-beta.208" />
    <PackageReference Include="TouchSocket.Core.DependencyInjection" Version="2.0.0-beta.208" />
  </ItemGroup>

</Project>
