<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="log4net" Version="3.1.0" />
    <PackageReference Include="TouchSocket" Version="3.7.2.41" />
    <PackageReference Include="TouchSocket.JsonRpc" Version="3.7.2.41" />
    <PackageReference Include="TouchSocket.Rpc" Version="3.7.2.41" />
    <PackageReference Include="TouchSocket.WebApi" Version="3.7.2.41" />
    <PackageReference Include="TouchSocket.XmlRpc" Version="3.7.2.41" />
    <PackageReference Include="TouchSocketPro.Dmtp" Version="3.7.2.41" />
  </ItemGroup>

  <ItemGroup>
    <None Update="App.config">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
